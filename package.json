{
  "name": "@thewhodidthis/otto-2d",
  "version": "0.0.7",
  "description": "Otto 2d",
  "keywords": [
    "automata",
    "cellular",
    "wolfram"
  ],
  "main": "index.js",
  "module": "index.es",
  "homepage": "https://github.com/thewhodidthis/otto-2d#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thewhodidthis/otto-2d.git"
  },
  "bugs": {
    "url": "https://github.com/thewhodidthis/otto-2d/issues"
  },
  "directories": {
    "example": "example"
  },
  "config": {
    "remote": "thewhodidthis@zazie:www/example/otto-2d"
  },
  "scripts": {
    "test": "node test",
    "lint": "eslint . example --ext .es test/*.js",
    "start": "rollup -c -e @thewhodidthis/otto && uglifyjs dist/otto2d.js -cmo dist/otto2d.min.js",
    "example": "npm run example:build && npm run example:serve",
    "example:build": "rollup -c -i example/index.es -f iife -o example/index.js",
    "example:serve": "php -S localhost:8000 -t example/",
    "example:press": "rsync -avruzLPC --delete --exclude=.DS_Store ./example/ $npm_package_config_remote"
  },
  "engines": {
    "node": ">=4.2.4",
    "npm": ">=3.5.3"
  },
  "dependencies": {
    "@thewhodidthis/otto": "github:thewhodidthis/otto"
  },
  "devDependencies": {
    "babel-eslint": "latest",
    "babel-plugin-external-helpers": "latest",
    "babel-preset-latest": "latest",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "latest",
    "eslint-plugin-import": "latest",
    "rollup": "latest",
    "rollup-plugin-babel": "latest",
    "rollup-plugin-node-resolve": "latest",
    "tape": "latest",
    "uglify-js": "latest"
  },
  "author": "sChzs BkgNns <sotirios.bakagiannis@gmail.com> (https://www.thewhodidthis.com)",
  "license": "Unlicense"
}
